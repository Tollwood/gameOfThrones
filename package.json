{
  "name": "got-tollwood",
  "productName": "got-tollwood",
  "version": "0.1.0",
  "scripts": {
    "compile": "webpack --config webpack.dev.config.js --progress --colors",
    "compile-test": "webpack --config webpack.test.config.js --progress --colors",
    "webpack:dev": "webpack --config webpack.dev.config.js --progress --colors",
    "webpack:dist": "webpack --config webpack.dist.config.js --progress --colors",
    "start": "node Server.js",
    "server:dev": "webpack-dev-server --config webpack.dev.config.js --open",
    "server:dist": "webpack-dev-server --config webpack.dist.config.js --open",
    "setupGameSize": "node ./scripts/setupGameSize.js",
    "assets:dev": "node ./scripts/generateAssetsClass.js --dev",
    "assets": "node ./scripts/generateAssetsClass.js",
    "test": "karma start karma.conf.js",
    "test-coverage": "karma start karma.coverage.conf.js --single-run",
    "test-single": "karma start karma.conf.js --single-run",
    "lint": "tslint src/**/*.ts -t verbose",
    "lint-fix": "tslint --fix **/*.ts -t verbose",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "postinstall": "npm run webpack:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tollwood/gameOfThrones.git"
  },
  "author": "Tobias Manger <https://github.com/tollwood>",
  "bugs": {
    "url": "https://github.com/Tollwood/gameOfThrones/issues"
  },
  "homepage": "https://github.com/Tollwood/gameOfThrones#readme",
  "dependencies": {
    "@types/node": "8.0.53",
    "@types/redux": "^3.6.0",
    "clean-webpack-plugin": "0.1.16",
    "commander": "2.9.0",
    "expose-loader": "0.7.3",
    "express": "^4.16.2",
    "file-loader": "0.10.1",
    "git-revision-webpack-plugin": "2.4.1",
    "html-webpack-plugin": "2.28.0",
    "phaser-ce": "2.7.5",
    "shelljs": "0.7.7",
    "ts-loader": "^2.0.3",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "typescript": "2.4.0",
    "typescript-map": "0.0.5",
    "webpack": "^2.3.1",
    "webpack-shell-plugin": "0.5.0",
    "xml2js": "0.4.17"
  },
  "devDependencies": {
    "@types/jasmine": "2.8.2",
    "coveralls": "^3.0.0",
    "karma": "^1.6.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.8",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^2.0.5",
    "phantomjs-prebuilt": "^2.1.14",
    "jasmine": "^2.6.0",
    "source-map-loader": "^0.2.1",
    "sourcemap-istanbul-instrumenter-loader": "^0.2.0",
    "webpack-dev-server": "2.4.2",

    "pngjs-image": "0.11.7"
  },
  "engines": {
    "node": "6.2.0",
    "npm": "5.4.2"
  }
